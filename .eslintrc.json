{
    "extends": "eslint:recommended",
    "env": {
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {},
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": 0,
        "quotes": [1, "double", { "avoidEscape": true, "allowTemplateLiterals": true }],
        "no-unused-vars": [1, { "args": "none", "varsIgnorePattern": "^_" }]
    }
}
