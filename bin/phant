#!/bin/bash
AR_PHANT_C1="\x1b[31m"
AR_PHANT_C2="\x1b[0m"
echo
echo -e "$AR_PHANT_C1        ████████    ████████"
echo -e "$AR_PHANT_C1    ██████████████████████  ██"
echo -e "$AR_PHANT_C1    ██████████████████████████"
echo -e "$AR_PHANT_C1  ██████████████████████████████"
echo -e "$AR_PHANT_C1  ██████████████████████$AR_PHANT_C2██$AR_PHANT_C1  ████"
echo -e "$AR_PHANT_C1  ████████████████████  $AR_PHANT_C2██$AR_PHANT_C1    ██"
echo -e "$AR_PHANT_C1  ████████████████████    $AR_PHANT_C2██$AR_PHANT_C1  ██"
echo -e "$AR_PHANT_C1  ██  ████████████            ██"
echo -e "$AR_PHANT_C1      ████    ████          ██"
echo -e "$AR_PHANT_C1      ████    ████"
echo